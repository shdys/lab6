<body>
<section id="content" class="m-t-lg wrapper-md animated fadeInUp">
  <div class="container aside-xxl"> <a class="navbar-brand block">Secret-Recipe</a>
    <section class="panel panel-default bg-white m-t-lg">
      <header class="panel-heading text-center"> <strong>Sign up</strong> </header>

					<form name="form2" id="ff2" method="post" action="Registration" class="panel-body wrapper-lg">
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Name :" name="userName" id="firstname" required  class="panel-body wrapper-lg">
						</div>
						<div class="form-group">
							<input type="tel" class="form-control" placeholder="Email Address :" name="userEmail" id="email2" required   class="panel-body wrapper-lg">
						</div>				
						<div class="form-group">
							<input name="userSex" id="gender" type="radio" value = "male" required  class="panel-body wrapper-lg"> Male <input name="userSex" id="gender" type="radio" value = "female" required  class="panel-body wrapper-lg"> Female 
						</div>				
						<div class="form-group">
							<input type="password" class="form-control" placeholder="Password :" name="userPassword" id="password" required class="panel-body wrapper-lg">
						</div>				
						<div class="form-group">
							<input type="password" class="form-control" placeholder="Retype Password :" name="rePassword" id="repassword" required class="panel-body wrapper-lg">
						</div>					
						<button onclick="return check1()" type="submit" name="registration" class="btn btn-primary">Sign up</button>
						<button type="reset" name="reset" class="btn btn-primary">Reset</button>
					</form>        
        
        <div class="line line-dashed"></div>
        <div class="line line-dashed"></div>
        <p class="text-muted text-center"><small>Already have an account?</small></p>
        <a href="signin.jsp" class="btn btn-default btn-block">Sign in</a>
      </form>
    </section>
  </div>
</section>
<!-- footer -->
<footer id="footer">
</footer>
